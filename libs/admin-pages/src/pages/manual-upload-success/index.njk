{% extends "layouts/base-template.njk" %}
{% from "govuk/components/panel/macro.njk" import govukPanel %}

{% block pageTitle %}{{ pageTitle }}{% endblock %}

{% block backLink %}{% endblock %}

{% block page_content %}
{{ govukPanel({
  titleText: title,
  html: uploadedMessage
}) }}

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">

    <h2 class="govuk-heading-m govuk-!-margin-top-4">{{ nextStepsHeading }}</h2>

    <ul class="govuk-list">
      <li>
        <a href="/manual-upload" class="govuk-link govuk-!-font-weight-bold">{{ uploadAnotherLink }}</a>
      </li>
      <li>
        <a href="/remove-list-search" class="govuk-link govuk-!-font-weight-bold">{{ removeFileLink }}</a>
      </li>
      <li>
        <a href="/admin-dashboard" class="govuk-link govuk-!-font-weight-bold">{{ homeLink }}</a>
      </li>
    </ul>

  </div>
</div>
{% endblock %}
